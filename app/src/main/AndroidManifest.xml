<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.proyectogassapp">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_app_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".InstruccionesActivity"></activity>
        <activity android:name=".CambiarCorreoActivity" />
        <activity android:name=".EliminarCuentaActivity" />
        <activity android:name=".RestablecerClaveActivity" />
        <activity android:name=".DetalleEstacionActivity" />
        <activity android:name=".EstacionesActivity" />
        <activity android:name=".AgregarTarjetaActivity" />
        <activity android:name=".MapaActivity" />
        <activity android:name=".PerfilActivity" />
        <activity android:name=".TarjetasActivity" />
        <activity android:name=".RegistrarActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>